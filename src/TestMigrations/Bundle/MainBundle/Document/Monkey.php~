<?php

namespace TestMigrations\Bundle\MainBundle\Document;

use Doctrine\ODM\MongoDB\Mapping\Annotations as ODM;

/**
 * @ODM\Document
 */
class Monkey
{
    /**
     * @ODM\Id
     */
    protected $id;

    /**
     * @ODM\String
     */
    protected $name;

    /**
     * @ODM\Collection
     * @ODM\ReferenceMany(targetDocument="TestMigrations\Bundle\MainBundle\Document\Hobby")
     */
    protected $hobbies;
}
